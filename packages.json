{
  "PACKAGES": {
    "pip3": {
      "termux": [
        "pip3 install --upgrade pip"
      ],
      "linux": [
        "sudo apt install python3-pip",
        "pip3 install --upgrade pip"
      ],
      "ish shell": [
        "apk add py3-pip",
        "pip3 install --upgrade pip"
      ]
    },
    "gcc": {
      "termux": [
        "pkg install clang"
      ],
      "linux": [
        "sudo apt install gcc",
        "sudo apt install pentium-builder"
      ],
      "ish shell": [
        "apk add gcc"
      ]
    },
    "nmap": {
      "termux": [
        "pkg install nmap"
      ],
      "linux": [
        "sudo apt install nmap"
      ],
      "ish shell": [
        "apk add nmap"
      ]
    },
    "openssl": {
      "termux": [
        "pkg install openssl",
        "pkg install openssl-tool"
      ],
      "linux": [
        "sudo apt install openssl"
      ],
      "ish shell": [
        "apk add openssl"
      ]
    }
  },
  "PYTHON3_MODULES": [
    "N4Tools",
    "rich",
    "uncompyle6",
    "pyfiglet",
    "python-bidi",
    "arabic-reshaper",
    "bs4",
